@article{costas-ferreiraToxicEffectsGlyphosate2022,
  title = {Toxic {{Effects}} of {{Glyphosate}} on the {{Nervous System}}: {{A Systematic Review}}},
  shorttitle = {Toxic {{Effects}} of {{Glyphosate}} on the {{Nervous System}}},
  author = {{Costas-Ferreira}, Carmen and Dur{\'a}n, Rafael and Faro, Lilian R. F.},
