  author = {Veaudor, Th{\'e}o and {Blanc-Garin}, Victoire and Chenebault, C{\'e}lia and {Diaz-Santos}, Encarnaci{\'o}n and Sassi, Jean-Fran{\c c}ois and {Cassier-Chauvat}, Corinne and Chauvat, Franck},
