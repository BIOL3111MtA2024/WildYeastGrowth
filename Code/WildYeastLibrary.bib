@article{berdynskiSOD1MutationsAssociated2022,
  title = {{{SOD1}} Mutations Associated with Amyotrophic Lateral Sclerosis Analysis of Variant Severity},
  author = {Berdy{\'n}ski, Mariusz and Miszta, Przemys{\l}aw and Safranow, Krzysztof and Andersen, Peter M. and Morita, Mitsuya and Filipek, S{\l}awomir and {\.Z}ekanowski, Cezary and {Ku{\'z}ma-Kozakiewicz}, Magdalena},
