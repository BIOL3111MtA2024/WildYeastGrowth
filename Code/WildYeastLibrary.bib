@article{longbottomInitialInvestigationsNovel2021,
  title = {Initial Investigations of a Novel Bioluminescence Method for Imaging Dental Demineralization},
  author = {Longbottom, Christopher and Vernon, Bruce and Perfect, Emma and Haughey, Anne-Marie and Christie, Adam and Pitts, Nigel},
  year = {2021},
  month = oct,
